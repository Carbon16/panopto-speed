{
  "manifest_version": 3,
  "name": "HyperPanoptoSpeed",
  "version": "1.1",
  "description": "Sets panopto video playback rate via a slider, up to 6x speed. For when you need to watch a semester's lectures in one afternoon.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Set Playback Speed",
    "default_icon": {
      "32": "icons/icon-32.svg"
    }
  }
  ,
  "icons": {
    "32": "icons/icon-32.svg"
  }
  ,
  "browser_specific_settings": {
    "gecko": {
      "id": "@panopto-speed",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "host_permissions": ["*://*.panopto.com/*"]
}